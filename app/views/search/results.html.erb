<%= render "search/search_box" %>

<div id="results">
    <table>
        <% for project in @projects %>
            <tr>
                <td><%= link_to project.uuid, project_path(project.id) %></td>
                <td><%= project.name %></td>
                <td><%= project.description %></td>
            </tr>
        <% end %>
    </table>
</div>

<%= paginate @projects %>
